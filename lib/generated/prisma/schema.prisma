// Try Prisma Accelerate: https://pris.ly/cli/accelerate-init

generator client {
  provider = "prisma-client-js"
  output   = "../lib/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Service {
  id          Int    @id @default(autoincrement())
  serviceName String
  description String
  image       String
}

model Team {
  id         Int    @id @default(autoincrement())
  name       String
  profession String
  bio_data   String
  image      String
}

model Project {
  id          Int    @id @default(autoincrement())
  category    String
  title       String
  description String
  image       String
}

model Thought {
  id           Int    @id @default(autoincrement())
  experts_name String
  bio_data     String
  thought      String
  image        String
}

model Contact {
  id        Int      @id @default(autoincrement())
  name      String
  email     String   @unique
  phone     String
  message   String
  createdAt DateTime @default(now())
}
